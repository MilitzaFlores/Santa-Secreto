{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"cloudformation:*",
				"s3:CreateBucket",
				"s3:PutObject",
				"s3:GetObject",
				"s3:DeleteObject",
				"s3:DeleteBucket",
				"s3:PutBucketPolicy",
				"s3:PutLifecycleConfiguration",
				"s3:GetBucketPolicy",
				"s3:GetBucketLocation",
				"s3:ListBucket",
				"s3:PutBucketTagging",
				"s3:PutEncryptionConfiguration",
				"s3:DeleteBucketPolicy"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"lambda:*",
				"logs:*",
				"iam:PassRole",
				"iam:GetRole",
				"iam:CreateRole",
				"iam:DeleteRole",
				"iam:AttachRolePolicy",
				"iam:DetachRolePolicy",
				"apigateway:PUT"
			],
			"Resource": "*"
		}
	]
}